		ListClass1.add("C++");
		ListClass1.add("Java");
		ListClass1.add("Scala");
		ListClass1.add(".Net");
		ListClass1.add("Python");
		ListClass1.add("PERL");
		ListClass1.add("PHP");
		ListClass1.add("Java Script");
		ListClass1.add("Ruby");
		add(ListClass1);
	}
	 
}
